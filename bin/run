#!/bin/sh
# Based on script generated by deno install. How to re-generate:
# 1. deno install --root . --name run -A --unstable ./src/app.ts
# 2. Modify the path to be relative.
deno "run" "--allow-read" "--allow-write" "--allow-net" "--allow-env" "--allow-run" "--allow-plugin" "--allow-hrtime" "--unstable" "./src/app.ts" "$@"
